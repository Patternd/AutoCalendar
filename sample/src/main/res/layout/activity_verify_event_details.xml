<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.alamkanak.weekview.sample.verifyEventDetails">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="103dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Event Title"
        android:textAlignment="viewStart"
        android:textAllCaps="true"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/v_title"
        app:layout_constraintTop_toBottomOf="@+id/textView26" />

    <TextView
        android:id="@+id/v_title"
        android:layout_width="209dp"
        android:layout_height="35dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="8dp"
        android:text="TextView"
        android:textAlignment="textEnd"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView26" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="103dp"
        android:layout_height="35dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="21dp"
        android:text="Priority"
        android:textAlignment="viewStart"
        android:textAllCaps="true"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.375"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/v_priority"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/v_priority"
        android:layout_width="209dp"
        android:layout_height="35dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="21dp"
        android:text="TextView"
        android:textAlignment="textEnd"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/v_title" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="103dp"
        android:layout_height="35dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="17dp"
        android:text="Location"
        android:textAlignment="viewStart"
        android:textAllCaps="true"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.375"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/v_location"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <TextView
        android:id="@+id/v_location"
        android:layout_width="209dp"
        android:layout_height="35dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="17dp"
        android:text="TextView"
        android:textAlignment="textEnd"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/v_priority" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="103dp"
        android:layout_height="35dp"
        android:layout_marginRight="28dp"
        android:layout_marginTop="17dp"
        android:text="Start Time"
        android:textAlignment="viewStart"
        android:textAllCaps="true"
        android:textStyle="bold"
        app:layout_constraintRight_toLeftOf="@+id/v_startTime"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="1.0" />

    <TextView
        android:id="@+id/v_startTime"
        android:layout_width="209dp"
        android:layout_height="35dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="17dp"
        android:text="TextView"
        android:textAlignment="textEnd"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/v_location" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="103dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="12dp"
        android:text="End Time"
        android:textAlignment="viewStart"
        android:textAllCaps="true"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/v_EndTime"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <TextView
        android:id="@+id/v_EndTime"
        android:layout_width="209dp"
        android:layout_height="35dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="12dp"
        android:text="TextView"
        android:textAlignment="textEnd"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/v_startTime" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="103dp"
        android:layout_height="35dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="15dp"
        android:text="Start Date"
        android:textAlignment="viewStart"
        android:textAllCaps="true"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <TextView
        android:id="@+id/v_startDate"
        android:layout_width="209dp"
        android:layout_height="35dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="15dp"
        android:text="TextView"
        android:textAlignment="textEnd"
        app:layout_constraintLeft_toRightOf="@+id/textView22"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/v_EndTime"
        app:layout_constraintHorizontal_bias="0.714" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="103dp"
        android:layout_height="35dp"
        android:layout_marginLeft="28dp"
        android:layout_marginTop="15dp"
        android:text="End Date"
        android:textAlignment="textStart"
        android:textAllCaps="true"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView22" />

    <TextView
        android:id="@+id/v_endDate"
        android:layout_width="209dp"
        android:layout_height="35dp"
        android:text="TextView"
        android:textAlignment="textEnd"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/v_startDate"
        app:layout_constraintLeft_toRightOf="@+id/textView24"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.714" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="328dp"
        android:layout_height="43dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="8dp"
        android:text="Verify your new event details. Is this correct?"
        android:textSize="18sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="119dp"
        android:layout_marginTop="31dp"
        android:text="FINISH"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/v_endDate" />

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginTop="31dp"
        android:text="CANCEL"
        app:layout_constraintHorizontal_bias="0.546"
        app:layout_constraintLeft_toRightOf="@+id/button6"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/v_endDate" />
</android.support.constraint.ConstraintLayout>