<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.alamkanak.weekview.sample.ViewEvent">

    <TextView
        android:id="@+id/titleText"
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:text="@string/titleText"
        tools:text="Title"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintRight_toLeftOf="@+id/title"
        android:layout_marginRight="52dp" />

    <TextView
        android:id="@+id/title"
        android:layout_width="215dp"
        android:layout_height="25dp"
        android:text="TextView"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/location"
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:text="@string/location"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/titleText"
        app:layout_constraintRight_toLeftOf="@+id/locView"
        android:layout_marginRight="52dp" />

    <TextView
        android:id="@+id/locView"
        android:layout_width="215dp"
        android:layout_height="25dp"
        android:text="TextView"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="25dp"
        app:layout_constraintTop_toBottomOf="@+id/title" />

    <TextView
        android:id="@+id/dateView"
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:text="@string/dateView"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/location"
        app:layout_constraintRight_toLeftOf="@+id/textView4"
        android:layout_marginRight="52dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="215dp"
        android:layout_height="25dp"
        android:text="TextView"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="28dp"
        app:layout_constraintTop_toBottomOf="@+id/locView" />

    <TextView
        android:id="@+id/startTime"
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:text="@string/startTime"
        android:layout_marginTop="22dp"
        app:layout_constraintTop_toBottomOf="@+id/dateView"
        app:layout_constraintRight_toLeftOf="@+id/startText"
        android:layout_marginRight="52dp" />

    <TextView
        android:id="@+id/startText"
        android:layout_width="215dp"
        android:layout_height="25dp"
        android:text="TextView"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="22dp"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/endTime"
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:text="@string/endTime"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/startTime"
        app:layout_constraintRight_toLeftOf="@+id/endText"
        android:layout_marginRight="52dp" />

    <TextView
        android:id="@+id/endText"
        android:layout_width="215dp"
        android:layout_height="25dp"
        android:text="TextView"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/startText"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent" />

    <TextView
        android:id="@+id/description"
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:text="@string/description"
        android:layout_marginTop="29dp"
        app:layout_constraintTop_toBottomOf="@+id/endTime"
        app:layout_constraintRight_toLeftOf="@+id/descripText"
        android:layout_marginRight="52dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/descripText"
        android:layout_width="215dp"
        android:layout_height="25dp"
        android:text="TextView"
        android:layout_marginTop="29dp"
        app:layout_constraintTop_toBottomOf="@+id/endText"
        android:layout_marginRight="16dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0" />
</android.support.constraint.ConstraintLayout>
